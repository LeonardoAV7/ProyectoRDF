    <dc:title>RadSem: Semantic Annotation and Retrieval for Medical Images</dc:title>
    <cofunc:keyword>Ontology-driven Metadata Extraction</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/sven-regel"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
