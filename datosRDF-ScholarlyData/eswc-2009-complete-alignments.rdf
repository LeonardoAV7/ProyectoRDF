    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/thanh-tran"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <rdfs:label>Semantic Wiki Search</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/peter-haase"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/daniel-herzig"/>
