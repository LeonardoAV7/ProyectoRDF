  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/22">
    <cofunc:keyword>Ontology Mapping</cofunc:keyword>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
