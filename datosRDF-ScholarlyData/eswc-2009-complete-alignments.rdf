    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
    <dc:title>Who the Heck is the Father of Bob?</dc:title>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Application"/>
