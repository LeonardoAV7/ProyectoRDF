    <dc:subject>ontology</dc:subject>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2009/paper/1"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/eswc/2009/proceedings"/>
