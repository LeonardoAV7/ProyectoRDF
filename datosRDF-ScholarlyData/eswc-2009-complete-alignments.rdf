    <dc:title>Can RDB2RDF Tools Feasibly Expose Large Science Archives for Data Integration?</dc:title>
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/163/authorList"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/163/authorList"/>
