  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2009/paper/34">
    <dc:subject>ontology driven method</dc:subject>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Annotation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
