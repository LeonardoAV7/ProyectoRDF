    <cofunc:keyword>decidability</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/chan-le-duc"/>
    <dc:title>Decidability of $\mathcal{SHI}$ with transitive closure of roles</dc:title>
    <cofunc:keyword>description logics</cofunc:keyword>
