    <dc:subject>RDF</dc:subject>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Optimization_(computer_science)"/>
    <cofunc:keyword>Ranking</cofunc:keyword>
    <cofunc:keyword>Query evaluation</cofunc:keyword>
