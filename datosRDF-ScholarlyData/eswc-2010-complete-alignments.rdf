    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/biswanath-dutta"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/feroz-farazi"/>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <dc:title>GeoWordNet: a resource for geo-spatial applications</dc:title>
