  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2010/paper/social_web/5">
    <dc:title>Semantics, Sensors, and the Social Web: The Live Social Semantics experiments</dc:title>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/ciro-cattuto"/>
