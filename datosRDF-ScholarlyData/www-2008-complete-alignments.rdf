  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/694">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/xiaoxun-zhang"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2008/paper/694"/>
