  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/462">
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/eric-horvitz"/>
    <dc:title>Planetary-Scale Views on a Large Instant-Messaging Network</dc:title>
