    <dc:title>Characterizing Typical and Atypical User Sessions in Clickstreams</dc:title>
    <dc:subject>clickstream analysis</dc:subject>
    <cofunc:title>Characterizing Typical and Atypical User Sessions in Clickstreams</cofunc:title>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
