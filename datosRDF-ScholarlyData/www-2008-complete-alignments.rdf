  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/706">
    <dc:title>Externalities in Online Advertising</dc:title>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <dc:subject>externalities</dc:subject>
