  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2008/paper/673">
    <dc:title>Trust-based recommendation systems: an axiomatic approach</dc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/jennifer-chayes"/>
    <dc:subject>Recommendation systems</dc:subject>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/moshe-tennenholtz"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/abraham-flaxman"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/www/2008/proceedings"/>
