    <dc:subject>repetitive pattern</dc:subject>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/wei-lai"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/rui-cai"/>
