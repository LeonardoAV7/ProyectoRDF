    <cofunc:title>Detecting Image Spam using Visual Features and Near Duplicate Detection</cofunc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/saurabh-nangia"/>
    <cofunc:keyword>machine learning</cofunc:keyword>
