    <cofunc:title>A graph-theoretic approach to webpage segmentation</cofunc:title>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/deepayan-chakrabarti"/>
