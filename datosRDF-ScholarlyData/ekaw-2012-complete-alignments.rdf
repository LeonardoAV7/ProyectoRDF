    <cofunc:title>User-Friendly Pattern-Based Transformation of OWL Ontologies</cofunc:title>
    <dc:title>User-Friendly Pattern-Based Transformation of OWL Ontologies</dc:title>
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
