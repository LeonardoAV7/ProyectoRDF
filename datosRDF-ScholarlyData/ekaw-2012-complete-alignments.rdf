  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ekaw2012/paper/research/9">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/ekaw2012/paper/research/9/authorList"/>
