  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/inuse/171">
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2009/proceedings"/>
