    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <dc:title>RKBPlatform: Opening up Services in the Web of Data</dc:title>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/ian-millard"/>
