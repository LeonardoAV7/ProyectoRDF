  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/poster_demo/158">
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/poster_demo/158/authorList"/>
