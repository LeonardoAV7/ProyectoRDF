    <dc:title>Queries to Hybrid MKNF Knowledge Bases through Oracular Tabling</dc:title>
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2009/paper/research/424/authorList"/>
