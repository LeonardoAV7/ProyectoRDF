    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/axel-polleres"/>
    <dc:title>Produce and Consume Linked Data with Drupal!</dc:title>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2009/proceedings"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2009/paper/inuse/101"/>
