    <cofunc:title>The Expressive Power of SPARQL</cofunc:title>
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
    <dc:title>The Expressive Power of SPARQL</dc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/renzo-angles"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
