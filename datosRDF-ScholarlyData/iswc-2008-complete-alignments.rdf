  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/87">
    <dc:subject>User Model</dc:subject>
    <dc:subject>Ontology</dc:subject>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/gerald-reif"/>
