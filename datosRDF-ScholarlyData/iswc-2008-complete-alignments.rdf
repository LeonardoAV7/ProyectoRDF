  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/13">
    <dc:title>A framework for semantic web policies</dc:title>
    <dc:subject>Semantic Web Policies</dc:subject>
    <cofunc:keyword>Trust negotiation</cofunc:keyword>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/piero-bonatti"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/luigi-sauro"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/13/authorList"/>
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/13/authorList"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/poster_demo/13"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/daniel-olmedilla"/>
