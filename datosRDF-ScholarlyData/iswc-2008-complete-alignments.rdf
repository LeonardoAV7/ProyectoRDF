  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/192">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/dominik-heim"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
    <dc:title>Evaluating Long-term use of the Gnowsis Semantic Desktop for PIM</dc:title>
    <cofunc:keyword>personal information management</cofunc:keyword>
    <dc:subject>semantic desktop</dc:subject>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/dominik-heim"/>
