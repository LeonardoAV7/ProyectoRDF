  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/251">
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/mark-a-musen"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/tania-tudorache"/>
