    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
    <cofunc:title>An ontology-based service-oriented application for mobility impaired users</cofunc:title>
    <dc:subject>Web services</dc:subject>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/dionysios-kehagias"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/dimitrios-tzovaras"/>
