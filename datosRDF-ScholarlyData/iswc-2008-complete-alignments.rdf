    <dc:subject>efficient service matchmaking</dc:subject>
    <rdfs:label>Efficient Semantic Web Service Discovery in Centralized and P2P Environments</rdfs:label>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/268/authorList"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/iswc/2008/paper/research/268"/>
