    <dc:title>Toward an Ontology for Finite Algebras</dc:title>
    <dc:subject>symbolic computing</dc:subject>
    <dc:subject>ontology</dc:subject>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
