  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/poster_demo/19">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
