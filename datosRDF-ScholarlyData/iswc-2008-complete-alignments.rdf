    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/ji-wang"/>
    <dc:subject>semantic web service</dc:subject>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/dengping-wei"/>
