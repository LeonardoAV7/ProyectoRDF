  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2008/paper/research/180">
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
    <rdfs:label>Extracting Semantic Constraint from Description Text for Semantic Web Service Discovery</rdfs:label>
    <cofunc:keyword>semantic constraint extraction</cofunc:keyword>
    <cofunc:keyword>semantic web service</cofunc:keyword>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
