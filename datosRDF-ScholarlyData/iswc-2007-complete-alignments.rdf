  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/409">
    <rdfs:label>Integrating Relations for a Domain Ontology</rdfs:label>
    <cofunc:title>Integrating Relations for a Domain Ontology</cofunc:title>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
