  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/575">
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/gang-wang"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Web_annotation"/>
