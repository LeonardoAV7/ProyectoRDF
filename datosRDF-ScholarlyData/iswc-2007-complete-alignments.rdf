    <dc:title>An Unsupervised Model for Exploring Hierarchical Semantics from Social Annotations</dc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/yong-yu"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/xian-wu"/>
