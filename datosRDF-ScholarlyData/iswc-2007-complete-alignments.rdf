  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/posters-demos/papers/385">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
