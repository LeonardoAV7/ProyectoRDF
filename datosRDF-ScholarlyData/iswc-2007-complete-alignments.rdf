  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/477">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Software_agent"/>
