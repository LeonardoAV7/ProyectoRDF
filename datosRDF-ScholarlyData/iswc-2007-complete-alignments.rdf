  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/547">
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Information_integration"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/giovanni-tummarello"/>
    <dc:title>Sindice.com: Weaving the Open Linked Data</dc:title>
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2007+aswc2007/tracks/research/papers/547/authorList"/>
    <cofunc:hasTopic rdf:resource="http://dbpedia.org/resource/Grid_computing"/>
