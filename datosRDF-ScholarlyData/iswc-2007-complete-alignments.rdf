    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/jung-hwa-choi"/>
    <dc:title>Ontology-based Semantic Context Modeling for Invisible Object Recognition</dc:title>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
