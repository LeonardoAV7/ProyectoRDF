  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/ruleml2011-europe/paper/32">
    <cofunc:keyword>Text Mining</cofunc:keyword>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/saeed-hassanpour"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/martin-oconnor"/>
