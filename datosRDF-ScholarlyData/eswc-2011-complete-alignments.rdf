  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2011/paper/linked-open-data/28">
    <cofunc:keyword>Optimization</cofunc:keyword>
    <rdfs:label>Zero-Knowledge Query Planning for an Iterator Implementation of Link Traversal Based Query Execution</rdfs:label>
    <dc:subject>Linked Data</dc:subject>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/eswc/2011/proceedings"/>
    <dc:subject>Heuristic</dc:subject>
