    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/nina-laurenne"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/jouni-tuominen"/>
