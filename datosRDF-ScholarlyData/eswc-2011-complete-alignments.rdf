  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2011/paper/inductive-approaches/8">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/kunal-verma"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/mariana-damova"/>
