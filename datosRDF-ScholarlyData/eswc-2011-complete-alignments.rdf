  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2011/paper/sensor-web/3">
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/kerry-taylor"/>
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
