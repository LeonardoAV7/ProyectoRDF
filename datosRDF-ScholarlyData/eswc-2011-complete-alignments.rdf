  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2011/paper/in-use/2">
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
    <bibo:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2011/paper/in-use/2/authorList"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
