  <rdf:Description rdf:about="https://w3id.org/scholarlydata/event/eswc2011/eswc/2011/keynote/2">
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/eswc/2011/keynote/2"/>
    <rdf:type rdf:resource="http://data.semanticweb.org/ns/swc/ontology#OrganisedEvent"/>
