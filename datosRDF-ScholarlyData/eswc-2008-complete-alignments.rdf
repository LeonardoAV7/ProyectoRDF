    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/martin-bloechl"/>
    <cofunc:keyword>sparql query processing</cofunc:keyword>
    <rdf:type rdf:resource="https://w3id.org/scholarlydata/ontology/conference-ontology.owl#InProceedings"/>
