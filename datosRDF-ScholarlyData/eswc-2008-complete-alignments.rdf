  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/eswc2008/paper/244">
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/eswc2008/paper/244/authorList"/>
