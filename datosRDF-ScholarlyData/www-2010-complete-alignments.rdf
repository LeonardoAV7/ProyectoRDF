    <dc:title>Inferring Relevant Social Networks from Interpersonal Communication</dc:title>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/munmun-de-choudhury"/>
    <j.1:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/1010/authorList"/>
