  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/254">
    <j.1:keyword>sub/super-documents</j.1:keyword>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
