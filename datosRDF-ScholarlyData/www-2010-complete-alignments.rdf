    <j.1:title>Detection and Analysis of Drive-by-Download Attacks and Malicious JavaScript Code</j.1:title>
    <j.1:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/673/authorList"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2010/paper/main/673"/>
