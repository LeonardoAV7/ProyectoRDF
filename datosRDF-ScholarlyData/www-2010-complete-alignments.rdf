  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/161">
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
