  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2010/paper/main/398">
    <dc:title>b-bit minwise hashing</dc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/ping-li"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2010/paper/main/398"/>
