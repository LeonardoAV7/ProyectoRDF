  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/162">
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
    <j.1:title>Navigation-Aided Retrieval</j.1:title>
    <j.1:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/www/2007/proceedings"/>
    <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
