  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/801">
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/collin-jackson"/>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/helen-wang"/>
