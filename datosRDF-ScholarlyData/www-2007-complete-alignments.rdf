  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/158">
    <j.1:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/158/authorList"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
