  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/555">
    <j.1:title>Exposing Private Information by Timing Web Applications</j.1:title>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/dan-boneh"/>
    <rdfs:label>Exposing Private Information by Timing Web Applications</rdfs:label>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/andrew-bortz"/>
    <owl:sameAs rdf:resource="http://data.semanticweb.org/conference/www/2007/paper/main/555"/>
