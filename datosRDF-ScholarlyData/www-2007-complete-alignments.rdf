  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/788">
    <dc:title>Visibly Pushdown Automata for Streaming XML</dc:title>
    <dc:creator rdf:resource="https://w3id.org/scholarlydata/person/mahesh-viswanathan"/>
    <j.6:authorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/www2007/paper/main/788/authorList"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
